{{template "base" .}}

{{define "content"}}
    <div class="container">
        <div class="row">
            <div class="col">
                <h1>Choose a Room</h1>

                {{$room := index .Data "rooms"}}

                {{range $room}}
                    {{.RoomName}} <br>
                {{end}}
            </div>
        </div>
    </div>
{{end}}